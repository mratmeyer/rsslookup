{
  "name": "rsslookup",
  "version": "1.5.0",
  "description": "A free tool to find the RSS feed for any website.",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "test": "vitest",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "NEXT_PUBLIC_CLOUDFLARE_TURNSTILE_SITE_KEY=0x4AAAAAABDQq80FHjTvJcsr opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "dependencies": {
    "@marsidev/react-turnstile": "^1.3.1",
    "@opennextjs/cloudflare": "^1.14.4",
    "htmlparser2": "^10.0.0",
    "next": "15.5.7",
    "next-seo": "^6.4.0",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-hot-toast": "^2.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^10.1.1",
    "postcss": "^8.4.8",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.0.23",
    "vitest": "^4.0.14",
    "wrangler": "^4.51.0"
  }
}
